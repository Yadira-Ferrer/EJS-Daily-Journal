<%- include("partials/header") %>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label class="form-label" for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle" />
    <label class="form-label" for="postContent">Content</label>
    <textarea class="form-control" name="postContent" rows="5"></textarea>
  </div>
  <button class="btn btn-primary" type="submit">Publish</button>
</form>
<%- include("partials/footer") %>
